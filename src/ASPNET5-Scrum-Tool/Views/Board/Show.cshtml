@model Boards

@section  css {
    <link rel="stylesheet" href="~/css/Board/Board.css" />
}

@section scripts {
    <script src='~/js/Board.js'> </script>
}
<h1 class="BoardNameHeading" id="@Model.ID">@Model.Name </h1>
<div class="row">

    <div class="col-md-11" id="MainColumn">
        @foreach (var column in Model.ColumnList)
        {
            @Component.Invoke("Panel_Lists",column )
        }
        <a id="AddColumnButton">Add a List</a>
    </div> <!-- Main Column End Div -->

    <div class="col-md-1" id="MenuColumn">
        <h1> Hello World </h1>

    </div> <!-- MenuColumn End -->

</div>